<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <security>
      <requestFiltering>
        <!-- This will handle requests up to ~2gb (in bytes) -->
        <requestLimits maxAllowedContentLength="2147483647" />
      </requestFiltering>
    </security>
    <!--<modules>
      <remove name="RequestFilteringModule"/>
    </modules>-->
  </system.webServer>
</configuration>